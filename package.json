{
  "name": "clear",
  "version": "1.0.0",
  "description": "[![Build Status](https://travis-ci.org/atlp-rwanda/space-bn-backend.svg?branch=develop)](https://travis-ci.org/atlp-rwanda/space-bn-backend)",
  "main": "index.js",
  "scripts": {
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "migrate:reset": "sequelize db:migrate:undo:all && npm run migrate",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --require @babel/register ./src/tests/ --recursive  --exit --timeout 20000 ",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
    "lint": "eslint ./",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "coverage": "nyc --reporter=lcov --reporter=text npm test",
    "start:dev": "nodemon --exec babel-node src/index.js",
    "client": "cd public/views && npm run serve",
    "start": "concurrently \"babel-node src/index.js\" \"cd public/views && npm run build && npm run serve \"",
    "build": "babel src --out-dir dist",
    "selenium": "cd src/selenium && node selenium.test.js",
    "artillery":"artillery run --output report.json artilery.yml && artillery report --output report.html report.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/space-bn-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/space-bn-backend/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/space-bn-backend#readme",
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-transform-regenerator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.10",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chromedriver": "^89.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "i18n": "^0.13.2",
    "joi": "^13.0.2",
    "jsonwebtoken": "^8.5.1",
    "mocha-lcov-reporter": "^1.3.0",
    "moment": "^2.29.1",
    "node-localstorage": "^2.1.6",
    "nodemon": "^2.0.6",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "pug": "^3.0.0",
    "selenium-webdriver": "^4.0.0-beta.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "artillery": "^1.7.2",
    "chai": "^4.2.0",
